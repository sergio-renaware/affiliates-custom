{
  "affiliate-template": {
    "children": [
      "rich-text#affiliate",
      "affiliate-store-name",
      "flex-layout.row#banner",
      "affiliate-profile-button",
      "search-result-layout.customQuery#affiliate"
    ]
  },

  "flex-layout.row#banner": {
    "children": ["image#affiliate-banner"]
  },

  "image#affiliate-banner": {
    "props": {
      "src": "https://renawareasesoracomercial.com/wp-content/uploads/2020/09/RENAW_Logo_PrimaryHoriz_Navy_RGB_JPEG-1024x269.jpg"
    }
  },

  "search-result-layout.customQuery#affiliate": {
    "props": {
      "querySchema": {
        "skusFilter": "ALL_AVAILABLE",
        "simulationBehavior": "default",
        "queryField": "137",
        "mapField": "productClusterIds",
        "facetsBehavior": "Dynamic"
      }
    },
    "blocks": [
      "search-result-layout.desktop",
      "search-result-layout.mobile",
      "search-not-found-layout"
    ]
  },

  "rich-text#affiliate": {
      "props": {
          "textAlignment": "CENTER",
          "textPosition": "CENTER",
          "text": "Esta es la tienda de ",
          "font": "t-heading-1"
      }
  }
}
